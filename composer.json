{
    "name": "sllh/php-cs-fixer-styleci-bridge",
    "description": "Auto configure PHP-CS-Fixer from StyleCI config file",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan SENECHAL",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "symfony/yaml": "~2.3",
        "symfony/dependency-injection": "~2.3"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "~1.9",
        "satooshi/php-coveralls": "~0.6",
        "codeclimate/php-test-reporter": "dev-master",
        "symfony/phpunit-bridge": "^2.7.4"
    },
    "autoload": {
        "psr-4": { "SLLH\\StyleCIBridge\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "SLLH\\StyleCIBridge\\Tests\\": "tests" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
